	<queryString><![CDATA[SELECT
     lote."cod_lote" AS lote_cod_lote,
     producto."nombre" AS producto_nombre,
     lote."formato" AS lote_formato,
     tipo_lote."tipo_lote" AS tipo_lote_tipo_lote,
     lote."cantidad_unidades" AS lote_cantidad_unidades,
     lote."dias_aprob" AS lote_dias_aprob,
     lote_produccion."fecha_revision" AS lote_produccion_fecha_revision
FROM
     "public"."lote" lote INNER JOIN "public"."lote_produccion" lote_produccion ON lote."cod_lote" = lote_produccion."id_lote"
     INNER JOIN "public"."producto" producto ON lote."cod_producto" = producto."cod_producto"
     INNER JOIN "public"."tipo_lote" tipo_lote ON lote."cod_tipo_lote" = tipo_lote."cod_tipo"
WHERE
    lote."cod_estado"=1 or lote."cod_estado"=2 and lote_produccion."revision"=false
ORDER BY lote."formato"]]></queryString>

